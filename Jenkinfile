pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git branch: 'main', url: 'https://github.com/Swethsira/mavenproject.git'
            }
        }

        stage('Build') {
            steps {
                dir('selenium-table-reader') {
                    echo 'Building the project...'
                    bat 'mvn clean install'
                }
            }
        }

        stage('Test') {
            steps {
                dir('selenium-table-reader') {
                    echo 'Running tests...'
                    bat 'mvn test'
                }
            }
        }
    }
}
